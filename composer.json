{
  "name": "mavs1971/yii2-app-advanced-startup-mavs",
  "description": "Yii 2 Practical Advanced Application Template (Startup Kit)",
  "keywords": [
    "yii2",
    "framework",
    "practical",
    "application template",
    "advanced"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "mavs",
      "email": "mvillalonga@gmail.com",
      "homepage": "http://mavs1971.github.io/yii2-app-advanced-startup-mavs",
      "role": "Developer"
    }
  ],

  "minimum-stability": "dev",
  "require": {
   "php": ">=5.6.0",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0"
    "mavs1971/yii2-mavsuser": "*",
    "abhi1693/yii2-config": "1.0.0",
    "abhi1693/yii2-installer": "*",
    "abhi1693/yii2-system-info": "*",
    "raoul2000/yii2-bootswatch-asset": "*",
    "kartik-v/yii2-widget-sidenav": "*",
    "kartik-v/yii2-widget-alert": "*",
    "wbraganca/yii2-dynamicform": "*"
  },
  "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
   "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
